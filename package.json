{
	"name": "emblem-playground",
	"description": "A playground for Emblem",
	"version": "0.0.1",
	"license": "GPL-3.0",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/jf908/emblem-playground"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"wasm": "wasm-pack build --release --target web src-wasm -- --no-default-features",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"github-markdown-css": "^5.2.0",
		"monaco-editor": "^0.36.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"unocss": "^0.50.1",
		"vite": "^4.0.0"
	},
	"type": "module"
}
